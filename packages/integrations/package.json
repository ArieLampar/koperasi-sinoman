{
  "name": "@koperasi-sinoman/integrations",
  "version": "1.0.0",
  "description": "External service integrations for Koperasi Sinoman including Midtrans, Cloudinary, and Fonnte",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./midtrans": {
      "types": "./dist/midtrans/index.d.ts",
      "import": "./dist/midtrans/index.mjs",
      "require": "./dist/midtrans/index.js"
    },
    "./cloudinary": {
      "types": "./dist/cloudinary/index.d.ts",
      "import": "./dist/cloudinary/index.mjs",
      "require": "./dist/cloudinary/index.js"
    },
    "./fonnte": {
      "types": "./dist/fonnte/index.d.ts",
      "import": "./dist/fonnte/index.mjs",
      "require": "./dist/fonnte/index.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/midtrans/index.ts src/cloudinary/index.ts src/fonnte/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/midtrans/index.ts src/cloudinary/index.ts src/fonnte/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "midtrans-client": "^1.3.1",
    "cloudinary": "^1.41.0",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@koperasi-sinoman/types": "workspace:*"
  }
}